pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- game area is 16 x 15 sprites
-- hence 240 bytes reserved
-- at start of map area for
-- building current screen
-- in
m1=0x2000

-- end of working map area
-- (17x16 due to half sprites
-- at screen edges)
m2=0x2110

-- test map data area
m3=0x4300

function setuptestmap()
 -- set up a test of a map
 l=m3
 poke(l,2) -- the start spr
 l+=1
-- l=pokemap(1,0,0,15,14,l)
 l=pokemap(0,2,0,1,14,l)
 l=pokemap(1,2,12,1,3,l)
 endmap(l)
end

function pokemap(s,x,y,w,h,loc)
 poke(loc,s) -- sprite
 poke(loc+1,x+y*16) -- top left
 poke(loc+2,w+h*16) -- wid, hei
 return loc+3
end

function endmap(loc)
 poke(loc,255)
 return loc+1
end

function fillbg(s)
 for x=0,17 do
  for y=0,16 do
   mset(x,y,s)
  end
 end 
end

function mapblit(x,ex,y,ey,s)
 for yy=y,ey do
 	for xx=x,ex do
 	 mset(xx,yy,s)
 	end
 end 	 
end

function mappaint()
	cls()
	clip(0,8,128,120)
	map(0,0,-4,4,17,16)
	clip()
end

function drawmap(l)
 bgspr=peek(l)
 print("bgspr="..bgspr)
 mapblit(0,0,16,15,bgspr)
 l+=1
 fillbg(bgspr)
	while(peek(l)!=255) do
		s=peek(l)
		xy=peek(l+1)
	 wh=peek(l+2)
	 x=band(xy,15)
	 y=flr(xy/16)
	 w=band(wh,15)
	 h=flr(wh/16)
	 print("xy wh="..x.." "..y.." "..w.." "..h)
  ex=x+w
  ey=y+h
  -- blit loop
  mapblit(x,ex,y,ey,s)
	 l+=3
	end
	mappaint()
end

setuptestmap()
drawmap(m3)


